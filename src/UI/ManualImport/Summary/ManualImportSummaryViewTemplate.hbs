<dl class="dl-horizontal">

    <dt>Path:</dt>
    <dd>{{file}}</dd>

    <dt>Artist:</dt>
    <dd>{{artist.name}}</dd>

    <dt>Album:</dt>
    <dd>{{album.title}}</dd>

    {{#each tracks}}
        <dt>Track:</dt>
        <dd>{{trackNumber}} - {{title}}</dd>
    {{/each}}

    <dt>Quality:</dt>
    <dd>{{quality.name}}</dd>
</dl>
